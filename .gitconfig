[user]
	name = alexandrecq
	email = adech@dolby.com
[core]
	editor = nvim
	excludesfile = ~/.gitignore_global

[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    df = diff -w
    dfno = diff --name-only
    lg = log --graph --oneline
    unstage = reset HEAD --
    last = log -1 HEAD
    set-upstream = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
[push]
	default = current
